HOLLOW SQUARE

#include<bits/stdc++.h>
 using namespace std;
 int main(){
     int n;
     int m;
     cin>>n;
     cin>>m;
     for(int i=1;i<=n;i++){
         for(int j=1;j<=m;j++){
             if(i==n||i==1 || j==1 || j==m){
                 cout<<"*";
             }
             else{
                 cout<<" ";
             }
         }
         cout<<endl;
     }
 }
